<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="unity" type="Microsoft.Practices.Unity.Configuration.UnityConfigurationSection, Microsoft.Practices.Unity.Configuration"/>
  </configSections>
  <unity>
    <container>
      <register type="FairFlights.DataConsumer.Interfaces.IDataSupplier, FairFlights.DataConsumer"
                mapTo="FairFlights.DataConsumer.Suppliers.SkyScanner, FairFlights.DataConsumer" />
    </container>
  </unity>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <add key="skyScannerUri" value="http://partners.api.skyscanner.net/apiservices/browsequotes/v1.0/"/>
    <add key="wegoUri" value="http://api.wego.com/flights/api/k/2/"/>
  </appSettings>
</configuration>